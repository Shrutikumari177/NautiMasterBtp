<mvc:View xmlns:form="sap.ui.layout.form"
    xmlns:table="sap.ui.table"
    xmlns:cards="sap.f.cards"
    controllerName="nauticalfe.controller.MastMarinePathUpd"
    xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns:core="sap.ui.core"
    xmlns:unified="sap.ui.unified"
    xmlns="sap.m"
    xmlns:tnt="sap.tnt"
>
    <Page id="page">
       <headerContent>
            <Bar id="_IDGenBar2">
                <contentLeft>
                    <core:Icon id="_IDGenIcon1"
                        src="sap-icon://nav-back"
                        press="onBackPress"
                    />
                    <core:Icon id="_IDGenIcon2"
                        src="sap-icon://home"
                        press="onPressHome"
                    />
                   
                    <Text id="_IDGenText1"
                        class="text1"
                        text="Port Location Upload"
                    />
                </contentLeft>
               
            </Bar>
        </headerContent>
        <Bar
            id="_IDGenBar1"
            class="_IDGenBar1"
        >
            <contentLeft>
                
                <Button
                    id="_IDGenButton2"
                    text="Download as Template"
                    press="onDownloadPress"
                />
                
            </contentLeft>
           
        </Bar>
        
        <HBox  class="upload">
        
           <Label text="File Name :" class="uploadlabel"/>
            <unified:FileUploader
                id="fileUploader"
                uploadUrl="/"
                buttonText="Choose File"
                fileType="xlsx"
                uploadComplete="onUploadComplete"
                change="onFileChange"
               class="uploadInput"
            />
            <Button
                text="Upload"
                press="onUploadPress"
            />
            <Button
                text="Preview"
                press="onPreviewPress"
            />
        </HBox>
 
        
    </Page>
</mvc:View>
 
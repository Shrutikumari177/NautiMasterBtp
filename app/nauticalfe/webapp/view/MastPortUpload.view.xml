<mvc:View
    xmlns:table="sap.ui.table"
    xmlns:cards="sap.f.cards"
    controllerName="nauticalfe.controller.MastPortUpload"
    xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns:core="sap.ui.core"
    xmlns:unified="sap.ui.unified"
    xmlns="sap.m"
    xmlns:tnt="sap.tnt"
>
    <Page id="page">
        <headerContent>
            <Bar id="_IDGenBar2">
                <contentLeft>
                    <core:Icon id="_IDGenIcon1"
                        src="sap-icon://nav-back"
                        press="onBackPress"
                    />
                    <core:Icon id="_IDGenIcon2"
                        src="sap-icon://home"
                        press="onBackPressHome"
                    />
                   
                    <Text id="_IDGenText1"
                        class="text1"
                        text="Port Upload"
                    />
                </contentLeft>
               
            </Bar>
        </headerContent>
        <Bar
            id="_IDGenBar1"
            class="_IDGenBar1"
        >
            <contentLeft>
                <!-- <Button
                    id="_IDGenButton1"
                    text="Save as Variant"
                    press="onSaveAs"
                /> -->
                <Button
                    id="_IDGenButton2"
                    text="Download as Template"
                    press="onDownloadPress"
                />
                <!-- <Button
                    id="button"
                    icon="sap-icon://slim-arrow-down"
                    iconFirst="false"
                    text="More"
                    press="onPress"
                    ariaHasPopup="Menu"
                /> -->
            </contentLeft>
           
        </Bar>
 
        <HBox alignContent="Stretch" class="upload">
            <unified:FileUploader
                id="fileUploader"
                uploadUrl="/"
                buttonText="Choose File"
                fileType="xlsx"
                uploadComplete="onUploadComplete"
                change="onFileChange"
            />
            <Button
                text="Upload"
                press="onUploadPress"
            />
            <Button
                text="Preview"
                press="onPreviewPress"
            />
        </HBox>
 
        
    </Page>
</mvc:View>
 